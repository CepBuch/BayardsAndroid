<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true"
        android:state_enabled="true" >
    <shape android:shape="rectangle">
      <solid android:color="#1D1259"></solid>
      <padding android:left="20dp"
         android:top="5dp"
         android:right="20dp"
         android:bottom="5dp" />
      <stroke
          android:width="1dip"
          android:color="#FFFFFF" />
      <corners android:radius= "8dp" />
    </shape>
  </item>
  <item android:state_enabled="false">
    <shape android:shape="rectangle">
      <solid android:color="#FFFFFF"></solid>
      <padding android:left="20dp"
         android:top="5dp"
         android:right="20dp"
         android:bottom="5dp" />
      <stroke
          android:width="1dip"
          android:color="#C0C3C4" />
      <corners android:radius= "8dp" />
    </shape>
  </item>
  <item>
    <shape android:shape="rectangle">
      <solid android:color="#FFFFFF"></solid>
      <padding android:left="20dp"
           android:top="5dp"
           android:right="20dp"
           android:bottom="5dp" />
      <stroke
          android:width="1dip"
          android:color="#1D1259" />
      <corners android:radius= "8dp" />
    </shape>
  </item>
</selector>